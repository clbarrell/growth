<%= select_tag "checkin_date", options_for_select([["Today - #{Time.zone.today.strftime("%a %b %e")}", "today"],["Yesterday - #{(Time.zone.today - 1.day).strftime("%a %b %e")}", "yesterday"], ["Day before yesterday - #{(Time.zone.today - 2.days).strftime("%a %b %e")}", "two days ago"]], "today"), { class: "form-control" } %>
