<div class="row">
  <div class="col-sm-12">
    <h3 class="text-center with-padding"><%= @goal %><br>
      <small>You've checked in
      <%= pluralize(@goal.checkin_count, "time") %></small></h3>
  </div>
</div>
<!-- QUESTION GRPAHS -->
<% @questions.each do |question| %>
  <div class="row">
    <h4 class="text-center">
      <%= question %>
    </h4>
  </div>
  <div class="row">
    <div class="graph-block">
      <p class="text-center">
        <% if question.scale != "Comment" %>
          <%= graph_question(question) %>
        <% else %>
          <dl class="dl-horizontal">
            <% question.comment_answers.order(created_at: :desc).each do |c| %>
              <dt><%= c.created_at.strftime('%h %d') %></dt>
              <dd><%= c.answer %></dd>
            <% end %>
          </dl>
        <% end %>
      </p>
    </div>
  </div>
<% end %>


<%= yield :charts_js %>
